{
  "compilerOptions": {
    "importHelpers": true,
    "target": "es2015",
    "jsx": "preserve",
    "noEmit": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~style": ["src/Asset/style.ts"],
      "~apis/*": ["src/Api/*"],
      "~store/*": ["src/Store"],
      "~components/*": ["src/Components/*"],
      "~fetch": ["src/Config/fetch.ts"],
      "~utils": ["src/Config/utils.ts"],
      "~navigation/*": ["src/Navigation/*"],
      "~pages/*": ["src/Page/*"]
    }
  },
  "exclude": ["node_modules"]
}
